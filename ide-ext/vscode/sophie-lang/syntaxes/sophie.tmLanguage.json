{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Sophie",
	"scopeName": "source.sophie",
	"patterns": [
		{
			"name": "keyword.control.sophie",
			"match": "(?i)\\b(as|case|esac|else|end|if|is|of|when|then|actor|role|to|do|cast|where|strict|self)\\b"
		}, {
			"name": "keyword.operator.sophie",
			"match": "(?i)\\b(and|or|not|div|mod|absurd)\\b"
		}, {
			"name": "keyword.other.sophie",
			"match": "(?i)\\b(import|type|assume|define|begin)\\s*:"
		}, {
			"name": "variable.language.sophie",
			"match": "(?i)\\b(my)\\b"
		}, {
			"name": "constant.language.sophie",
			"match": "(?i)\\b(yes|no)\\b"
		}, {
			"name": "string.quoted.single.sophie",
			"begin": "'",
			"end": "'"
		}, {
			"name": "string.quoted.double.sophie",
			"begin": "\"",
			"end": "\""
		}, {
			"name": "comment.line.number-sign",
			"match": "#.*$"
		}, {
			"name": "constant.numeric.sophie",
			"match": "\\b(0|\\d(_?\\d)*)(\\.\\d(_?\\d)*)?([eE][-+]?(0|\\d(_?\\d)*))?\\b"
		}, {
			"name": "support.constant.sophie",
			"match": "\\b(nil|cons|nope|this|done|step|less|same|more)\\b"
		}, {
			"name": "support.type.sophie",
			"match": "\\b(string|flag|number|list|maybe|entry|folio|pair|order|predicate)\\b"
		}, {
			"name": "support.class.sophie",
			"match": "\\b(Console|FileSystem)\\b"
		}, {
			"name": "support.variable.sophie",
			"match": "\\b(console|filesystem)\\b"
		}
	]
}